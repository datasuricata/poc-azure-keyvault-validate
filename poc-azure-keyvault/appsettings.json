{
  "Settings": {
    //usado para compor a chave base64
    "AccountName": "accountName",
    "AccountKey": "accountKey",

    //usado para acessar o cliente secret
    "ClientId": "clientId",
    "ClientSecret": "clientSecret",

    //usado para acessar o container
    "Container": "containerName",

    //usado para validar a chave rsa
    "Endpoint": "https://keyvault.azure.net/keys/TestKey/0000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}